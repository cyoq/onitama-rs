  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'board_plugin'",
      "cargo": {
        "args": ["test", "--no-run", "--lib", "--package=board_plugin"],
        "filter": {
          "name": "board_plugin",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'onitama-rs'",
      "cargo": {
        "args": ["build", "--bin=onitama-rs", "--package=onitama-rs"],
        "filter": {
          "name": "onitama-rs",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}",
      "env": {
        "CARGO_MANIFEST_DIR": "${workspaceFolder}"
      }
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'onitama-rs'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=onitama-rs",
          "--package=onitama-rs"
        ],
        "filter": {
          "name": "onitama-rs",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}
